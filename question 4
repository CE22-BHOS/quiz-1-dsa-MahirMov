void bubble(int arr[], int size) {
    int temp = 0;
    for (int i = 0; i < size - 1; i++) {
        for (int j = size - 1; j >= i + 1; j--) {
            if (arr[j] < arr[j - 1]) {
                temp = arr[j - 1];
                arr[j - 1] = arr[j];
                arr[j] = temp;
            }
        }
    }
}
